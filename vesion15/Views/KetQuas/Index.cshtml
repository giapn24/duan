@model IEnumerable<vesion15.Models.KetQua>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.HoSo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Diem)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HienThi)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.HoSo.MaHS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Diem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HienThi)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaKQ">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaKQ">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaKQ">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
